	<parameter name="PARAM_toSection" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT
     person.`firstName` AS FirstName,
     person.`gender` AS Gender,
     person.`gradeLevel` AS GradeLevel,
     person.`lastName` AS LastName,
     person.`middleInitial` AS MI,
     person.`schoolYear` AS SY,
     person.`studentNumber` AS StudentNumber,
     person.`toSection` AS ToSection
FROM
     `person` person
WHERE
	person.section = $P{PARAM_toSection} AND
	(person.`gender` != 'MALE' OR gender NOT LIKE 'B%')
AND
	person.`status` = 'ENROLLED'
AND
	personType = 'STUDENT'
ORDER BY
	person.`lastName`]]></queryString>

