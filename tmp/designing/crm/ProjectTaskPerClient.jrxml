     project.`company` AS Company,
     project.`product` AS Product,
     project.`startDate` AS StartDate,
     project.`completionDate` AS CompletionDate,
     project.`description` AS Description,
     projecttask.`manDays` AS ManDays,
     projecttask.`employee` AS Employee,
     projecttask.`amount` AS Amount,
     projecttask.`startDate` AS TStartDate,
     projecttask.`taskType` AS TaskType,
     projecttask.`status` AS Status,
     projecttask.`priority` AS Priority,
     projecttask.`description` AS TDescription,
     projecttask.`seq` AS Seq
FROM
     `projecttask` projecttask INNER JOIN `project` project ON projecttask.`projectName` = project.`projectName`
WHERE
     project.`companyId` = $P{REPORT_ADD_WHERE} AND
     (project.completionDate is null OR project.completionDate >= curdate())
ORDER BY
     project.`projectName`, projecttask.`startDate`, projecttask.`priority`]]></queryString>

