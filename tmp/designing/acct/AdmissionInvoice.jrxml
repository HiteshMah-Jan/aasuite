     patient_transaction.amountShoulderedByInsurance AS patient_transaction_amountShoulderedByInsurance,
     patient_transaction.receiptDate AS patient_transaction_receiptDate,
     patient_transaction.computedOverallDiscount AS patient_transaction_computedOverallDiscount,
     patient_transaction.admissionDate AS patient_transaction_admissionDate,
     patient_transaction.roomRate AS patient_transaction_roomRate,
     patient_transaction.receiptNumber AS patient_transaction_receiptNumber,
