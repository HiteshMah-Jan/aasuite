     patient_billing_item.useDate AS patient_billing_item_useDate,
     patient_transaction.department AS patient_transaction_department,
     patient_transaction.totalFee AS patient_transaction_totalFee,
     patient_transaction.otherFee AS patient_transaction_otherFee,
