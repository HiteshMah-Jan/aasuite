	<queryString><![CDATA[SELECT
     person.lastName AS person_lastName,
     person.gender AS person_gender,
     person.firstName AS person_firstName,
     patient_billing_item.discountReason AS patient_billing_item_discountReason,
     patient_billing_item.net_amount AS patient_billing_item_net_amount,
     patient_billing_item.amount AS patient_billing_item_amount,
     patient_billing_item.reason AS patient_billing_item_reason,
