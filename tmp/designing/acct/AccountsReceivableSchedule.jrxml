	<queryString><![CDATA[SELECT
     person.`middleInitial` AS MI,
     person.`lastName` AS LastName,
     person.`firstName` AS FirstName,
     person_A.`firstName` AS A_FirstName,
     person_A.`lastName` AS A_LastName,
     person_A.`middleInitial` AS A_MI,
     section.`gradeLevel` AS GradeLevel,
     payment.`paymentFor` AS PaymentFor,
     payment.`dueDate` AS DueDate,
     payment.`discount` AS Discount,
     payment.`schoolYear` AS SY,
     payment.`paid` AS Paid,
     payment.`overallBalance` AS OverallBalance,
     payment.`balance` AS Balance,
     payment.`surcharge` AS Surcharge,
     payment.`amount` AS Amount,
     payment.`section` AS Section,
     payment.`overallAmountDue` AS OverallAmountDue
FROM
     `section` section INNER JOIN `person` person_A ON section.`facultyId` = person_A.`personId`
     INNER JOIN `payment` payment ON section.`code` = payment.`section`
     INNER JOIN `person` person ON payment.`paidBy` = person.`personId`
WHERE
     payment.`paid` <> 1
 AND payment.`dueDate` Between $P{PARAM_startDate}
     And $P{PARAM_endDate}
ORDER BY
     section.gradeLevel ASC,
     payment.section ASC,
     person.lastName ASC]]></queryString>

