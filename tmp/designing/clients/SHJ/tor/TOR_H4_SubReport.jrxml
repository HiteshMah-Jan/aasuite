	<queryString><![CDATA[SELECT
     enrollment.`seq` AS seq,
     enrollment.`studentId` AS studentId,
     enrollment.`schoolYear` AS schoolYear,
     enrollment.`recommendation` AS recommendation,
     enrollment.`section` AS section,
