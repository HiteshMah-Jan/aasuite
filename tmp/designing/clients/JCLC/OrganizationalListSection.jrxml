	<queryString><![CDATA[SELECT
     person.`status` AS person_status,
     person.`firstName` AS person_firstName,
     person.`middleInitial` AS person_middleInitial,
     person.`studentNumber` AS person_studentNumber,
     person.`gradeLevel` AS person_gradeLevel,
     person.`lastName` AS person_lastName,
     person.`section` AS person_section,
     section.`startTime` AS section_startTime,
     section.`endTime` AS section_endTime,
     person.`personId` AS person_personId,
     if(person.`gender`='MALE','Boys','Girls')AS person_gender,
     section.`startTime` AS startTime,
     section.`endTime` AS endTime,
     person.`gradeLevelDesc` AS person_gradeLevelDesc
FROM
     `section` section INNER JOIN `person` person ON section.`code` = person.`section`
WHERE
     person.`status` = 'ENROLLED' and 
      person.gradeLevel is not null AND 
      person.gradeLevel!='' and 
      person.section is not null AND 
      person.section!=''
 and person.section LIKE '$P!{PARAM_section}%'

ORDER BY
	person.`gradeLevel`,person.`section`,person_gender,person.lastName]]></queryString>

