	<queryString><![CDATA[SELECT
     person.`firstName` AS person_firstName,
     person.`lastName` AS person_lastName,
     person.`schoolYear` AS person_schoolYear,
     person.`section` AS person_section,
     person.`middleInitial` AS person_middleInitial,
     person.`gradeLevel` AS person_gradeLevel,
     section.`facultyId` AS section_facultyId,
     person.`personId` AS person_personId,
     person.`studentNumber` AS person_studentNumber,
     person.`toSection` AS person_toSection,
     person.`gradeLevelDesc` AS person_gradeLevelDesc,
     section.`faculty` AS section_faculty,
     enrollment.`schoolYear` AS enrollment_schoolYear,
     studentvaluesgrading.`comment1` AS studentvaluesgrading_comment1,
     studentvaluesgrading.`comment2` AS studentvaluesgrading_comment2,
     studentvaluesgrading.`comment3` AS studentvaluesgrading_comment3,
     studentvaluesgrading.`comment4` AS studentvaluesgrading_comment4,
     studentvaluesgrading.`status` AS studentvaluesgrading_status,
     person.`promotedTo` AS person_promotedTo
FROM
     `section` section INNER JOIN `person` person ON section.`code` = person.`section`
     INNER JOIN `enrollment` enrollment ON person.`personId` = enrollment.`studentId`
     INNER JOIN `studentvaluesgrading` studentvaluesgrading ON enrollment.`studentId` = studentvaluesgrading.`studentId`
WHERE
     person.personId = $P{REPORT_ADD_WHERE}
     and enrollment.schoolYear = "2009-2010"]]></queryString>

