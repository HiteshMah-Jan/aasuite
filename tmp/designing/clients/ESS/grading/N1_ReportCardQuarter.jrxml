	<queryString><![CDATA[SELECT
     person.`personId` AS person_personId,
     person.`firstName` AS person_firstName,
     person.`middleInitial` AS person_middleInitial,
     person.`lastName` AS person_lastName,
     person.`latestSchoolYear` AS person_latestSchoolYear,
     person.`gradeLevel` AS person_gradeLevel,
     person.`studentNumber` AS person_studentNumber,
     person.`section` AS person_section,
     person.`status` AS person_status,
     section.`faculty` AS section_faculty,
     person.`gradeLevelDesc` AS person_gradeLevelDesc,
     studentvaluesgrading.`totalDays2` AS studentvaluesgrading_totalDays2,
     studentvaluesgrading.`totalDays` AS studentvaluesgrading_totalDays,
     studentvaluesgrading.`totalDays3` AS studentvaluesgrading_totalDays3,
     studentvaluesgrading.`present` AS studentvaluesgrading_present,
     studentvaluesgrading.`absent` AS studentvaluesgrading_absent,
     studentvaluesgrading.`present2` AS studentvaluesgrading_present2,
     studentvaluesgrading.`absent2` AS studentvaluesgrading_absent2,
     studentvaluesgrading.`present3` AS studentvaluesgrading_present3,
     studentvaluesgrading.`absent3` AS studentvaluesgrading_absent3,
     studentvaluesgrading.`totalDays4` AS studentvaluesgrading_totalDays4,
     studentvaluesgrading.`present4` AS studentvaluesgrading_present4,
     studentvaluesgrading.`absent4` AS studentvaluesgrading_absent4,
     studentvaluesgrading.`tardy` AS studentvaluesgrading_tardy,
     studentvaluesgrading.`tardy2` AS studentvaluesgrading_tardy2,
     studentvaluesgrading.`tardy3` AS studentvaluesgrading_tardy3,
     studentvaluesgrading.`tardy4` AS studentvaluesgrading_tardy4,
     studentvaluesgrading.`comment1` AS studentvaluesgrading_comment1,
     studentvaluesgrading.`comment2` AS studentvaluesgrading_comment2,
     studentvaluesgrading.`comment3` AS studentvaluesgrading_comment3,
     person.`addStatus` AS person_addStatus
FROM
     `section` section INNER JOIN `person` person ON section.`code` = person.`section`
     INNER JOIN `studentvaluesgrading` studentvaluesgrading ON person.`personId` = studentvaluesgrading.`studentId`
     AND studentvaluesgrading.`gradeLevel` = section.`gradeLevel`
WHERE
     person.personId = $P{REPORT_ADD_WHERE}]]></queryString>

