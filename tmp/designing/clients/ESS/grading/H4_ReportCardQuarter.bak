     studentvaluesgrading.`present2` AS studentvaluesgrading_present2,
     studentvaluesgrading.`absent2` AS studentvaluesgrading_absent2,
     studentvaluesgrading.`present3` AS studentvaluesgrading_present3,
     studentvaluesgrading.`absent3` AS studentvaluesgrading_absent3,
     studentvaluesgrading.`totalDays4` AS studentvaluesgrading_totalDays4,
     studentvaluesgrading.`present4` AS studentvaluesgrading_present4,
     studentvaluesgrading.`absent4` AS studentvaluesgrading_absent4,
     studentvaluesgrading.`tardy` AS studentvaluesgrading_tardy,
