     studentvaluesgrading.`tardy3` AS studentvaluesgrading_tardy3,
     studentvaluesgrading.`tardy4` AS studentvaluesgrading_tardy4,
     person.`addStatus` AS person_addStatus
FROM
     `section` section INNER JOIN `person` person ON section.`code` = person.`section`
     INNER JOIN `studentvaluesgrading` studentvaluesgrading ON person.`personId` = studentvaluesgrading.`studentId`
WHERE
     person.personId = $P{REPORT_ADD_WHERE}]]></queryString>

