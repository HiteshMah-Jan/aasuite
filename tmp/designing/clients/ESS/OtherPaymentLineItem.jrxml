	<queryString><![CDATA[SELECT
     if(cancelled!=1,(invoice.totalAmount),0)AS PaymentAmount,
     otherpayment.`schoolYear` AS SY,
     otherpayment.`orDate` AS ORDate,
  otherpaymentlineitem.`otherPayment` AS OtherPayment,
     otherpayment.`cashier` AS Cashier,
     otherpayment.`orNumber` AS ORNumber,
     otherpayment.`orType` AS otherpayment_orType,
     invoice.`payer` AS Payer,
     otherpayment.`gradeLevel` AS gradeLevel,
     otherpayment.`completeDesc` AS Description
FROM
     `otherpayment` otherpayment INNER JOIN `otherpaymentlineitem` otherpaymentlineitem ON otherpayment.`seq` = otherpaymentlineitem.`otherPaymentId`
     INNER JOIN `invoice` invoice ON otherpayment.`invoiceId` = invoice.`seq`
WHERE
     otherpaymentlineitem.`otherPayment` LIKE '$P!{PARAM_type}%'
AND  invoice.cancelled = 0
AND  otherPayment.`orDate` BETWEEN $P{PARAM_startDate} AND $P{PARAM_endDate}]]></queryString>

