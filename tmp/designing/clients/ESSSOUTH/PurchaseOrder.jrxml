	<queryString><![CDATA[SELECT
     purchaseorder.`invoiceDate` AS InvoiceDate,
     purchaseorder.`invoiceNumber` AS InvoiceNumber,
     purchaseorder.`purchaseDate` AS PurchaseDate,
     purchaseorder.`totalPOAmount` AS TotalPOAmount,
     person.`companyName` AS CompanyName,
     purchaseorder.`remarks` AS Remarks,
     person_A.`middleInitial` AS MI,
     person_A.`lastName` AS LastName,
     person_A.`firstName` AS FirstName,
     purchaseorder.`approvedBy` AS ApprovedBy,
     purchaseorder.`approvedDate` AS ApprovedDate,
     purchaseorder.`requestedBy` AS RequestedBy,
     purchaseorder.`requestedDate` AS RequestedDate,
     purchaseorder.`receivedBy` AS ReceivedBy,
     purchaseorder.`receivedDate` AS ReceivedDate
FROM
     `person` person INNER JOIN `purchaseorder` purchaseorder ON person.`personId` = purchaseorder.`purchaseFrom`
     INNER JOIN `person` person_A ON purchaseorder.`purchaseBy` = person_A.`personId`
WHERE
 purchaseorder.`purchaseDate` BETWEEN $P{PARAM_startDate} AND $P{PARAM_endDate}
ORDER BY
     purchaseorder.`purchaseDate` ASC,
     person.`companyName` ASC]]></queryString>

