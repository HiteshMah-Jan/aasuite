	<queryString><![CDATA[SELECT
     enrollment.`gradeLevel` AS GradeLevel,
     payment.`paymentFor` AS Particulars,
     payment.`balance` AS Balance,
     payment.`dueDate` AS DueDate,
     payment.`section` AS Section,
     payment.`schoolYear` AS SY,
     payment.`amount` AS Debit,
     payment.`overallAmountDue` AS TotalBalance,
     payment.`overallAmountPaid` AS Credit,
     payment.`surchargePaid` AS SurchargePaid,
     payment.`paid` AS Paid,
     payment.`discount` AS payment_discount,
     payment.`orNumber` AS payment_orNumber,
     payment.`orDate` AS payment_orDate,
     payment.`orAmount` AS payment_orAmount,
     payment.`surcharge` AS Surcharge,
     payment.`accountNumber1` AS AccountNumber1,
     payment.`accountNumber2` AS AccountNumber2,
     payment.`accountNumber3` AS AccountNumber3,
     payment.`orType` AS ORType
FROM
     `enrollment` enrollment INNER JOIN `payment` payment ON enrollment.`studentId` = payment.`paidBy`
     AND payment.`recordId` = enrollment.`seq`
WHERE
     payment.schoolYear = $P{PARAM_schoolYear}
 AND payment.paidBy = $P{REPORT_ADD_WHERE}
 AND payment.`paid` = 1]]></queryString>

