	<queryString><![CDATA[SELECT
     payment.`dueDate` AS payment_dueDate,
     payment.`amount` AS payment_amount,
     payment.`balance` AS payment_balance,
     payment.`paymentFor` AS payment_paymentFor,
     payment.`surcharge` AS payment_surcharge,
     payment.`schoolYear` AS payment_schoolYear,
     payment.`payer` AS payment_payer,
     enrollment.`section` AS enrollment_section,
     person.`firstName` AS person_firstName,
     person.`lastName` AS person_lastName
FROM
     `payment` payment INNER JOIN `enrollment` enrollment ON payment.`recordId` = enrollment.`seq`
     INNER JOIN `section` section ON enrollment.`section` = section.`code`
     LEFT OUTER JOIN `person` person ON section.`facultyId` = person.`personId`
WHERE 
     payment.dueDate BETWEEN $P{PARAM_startDate} AND $P{PARAM_endDate}]]></queryString>

