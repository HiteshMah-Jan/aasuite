	<queryString><![CDATA[SELECT
     librarybooks.title AS Title,
     librarybooks.subtitle AS Subtitle,
     librarybooks.bookDescription AS BookDescription,
     librarybooks.barcode AS Barcode,
     book_shelf.shelf AS Shelf,
     book_publisher.publisherName AS PublisherName,
     librarybooks.isbn AS Isbn,
     borrowed_books.dueDate AS DueDate,
     person.lastName AS LastName,
     person.middleInitial AS MI,
     person.firstName AS FirstName,
     section.section AS Section,
     person.personType AS PersonType,
     course.courseName AS CourseName,
     borrowed_books.isReturned AS IsReturned,
     borrowed_books.dateReturned AS DateReturned,
     borrowed_books.dateBorrowed AS DateBorrowed,
     person_A.middleInitial AS A_middleInitial,
     person_A.lastName AS A_lastName,
     person_A.firstName AS A_firstName,
     person.contactNumber AS ContactNumber,
     person.mobilePhone AS MobilePhone
FROM
     book_shelf book_shelf INNER JOIN librarybooks librarybooks ON book_shelf.shelf = librarybooks.shelfOrGroup
     INNER JOIN book_publisher book_publisher ON librarybooks.publisher = book_publisher.publisherName
     INNER JOIN borrowed_books borrowed_books ON librarybooks.isbn = borrowed_books.isbn
     INNER JOIN person person ON borrowed_books.personId = person.personId
     INNER JOIN person person_A ON borrowed_books.librarian = person_A.personId
     INNER JOIN section section ON person.section = section.code
     INNER JOIN course course ON person.course = course.code]]></queryString>

