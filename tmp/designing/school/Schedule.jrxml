	<queryString><![CDATA[SELECT
     schedule.sched_day AS S_Day,
     person.firstName AS FirstName,
     person.lastName AS LastName,
     schedule.schedTimeEnd AS SchedTimeEnd,
     schedule.schedTime AS SchedTime,
     subject.subject AS Subject,
     course.courseName AS Grade_Level,
     section.code AS Section,
     person.middleInitial AS MI,
     room.code AS Room,
     building.code AS Building
FROM
     person person INNER JOIN schedule schedule ON person.personId = schedule.facultyId
     INNER JOIN subject subject ON schedule.subject = subject.code
     INNER JOIN course course ON schedule.course = course.code
     INNER JOIN section section ON schedule.blockOrSection = section.code
     INNER JOIN room room ON schedule.room = room.code
     INNER JOIN building building ON room.building = building.code]]></queryString>

