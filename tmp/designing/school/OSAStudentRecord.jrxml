	<queryString><![CDATA[SELECT
     studentencounter.offense AS studentencounter_offense,
     studentencounter.actionTaken AS studentencounter_actionTaken,
     studentencounter.isMajorOffense AS studentencounter_isMajorOffense,
     studentencounter.dateEncounter AS studentencounter_dateEncounter,
     section.facultyId AS section_facultyId,
     person.address AS person_address,
     person.contactNumber AS person_contactNumber,
     person.lastName AS person_lastName,
     person.firstName AS person_firstName,
     person_A.lastName AS person_A_lastName,
     person_A.firstName AS person_A_firstName,
     enrollment.section AS enrollment_section,
     person.personId AS person_personId,
     enrollment.studentId AS enrollment_studentId
FROM
     section section INNER JOIN person person_A ON section.facultyId = person_A.personId
     INNER JOIN enrollment enrollment ON section.code = enrollment.section
     INNER JOIN studentencounter studentencounter ON enrollment.studentId = studentencounter.studentId
     INNER JOIN person person ON enrollment.studentId = person.personId
WHERE
     person.personId = $P{REPORT_ADD_WHERE}]]></queryString>

